#	DEPDIR=$(DEPDIR) $(CCDEPMODE) $(depcomp) \
#	$(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(familyqueue_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -c -o familyqueue-familyqueue.obj `if test -f 'familyqueue.c'; then $(CYGPATH_W) 'familyqueue.c'; else $(CYGPATH_W) '$(srcdir)/familyqueue.c'; fi`

