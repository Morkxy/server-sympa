	if $(CC) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) $(queue_CPPFLAGS) $(CPPFLAGS) $(AM_CFLAGS) $(CFLAGS) -MT queue-queue.o -MD -MP -MF "$(DEPDIR)/queue-queue.Tpo" -c -o queue-queue.o `test -f 'queue.c' || echo '$(srcdir)/'`queue.c; \
	then mv -f "$(DEPDIR)/queue-queue.Tpo" "$(DEPDIR)/queue-queue.Po"; else rm -f "$(DEPDIR)/queue-queue.Tpo"; exit 1; fi
